{
    "name": "Finistere/colissimo-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a wrapper for the lexik/ws-colissimo library",
    "keywords": ["Symfony2", "bundle", "colissimo"],
    "homepage": "https://github.com/Finistere/LexikColissimoBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Cabot",
            "email": "n.cabot@lexik.fr"
        },
        {
            "name": "Dev Lexik",
            "email": "dev@lexik.fr"
        }
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/Finistere/ws-colissimo"
      }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.1",
        "Finistere/ws-colissimo": "dev-master"
    },
    "autoload":     {
        "psr-0": { "Lexik\\Bundle\\ColissimoBundle": "" }
    },
    "target-dir": "Lexik/Bundle/ColissimoBundle"
}
